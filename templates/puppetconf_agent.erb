
[agent]
  server = <%= @puppetmaster %>
  masterport = <%= @puppetmasterport %>

  logdir = <%= @logdir %>
  rundir = /var/run/puppet

  show_diff =<%= @showdiff %>
<% if defined?(@waitforcert) -%>
  waitforcert=<%= @waitforcert %>
<% end -%>
<% if defined?(@runinterval) -%>
  runinterval=<%= @runinterval %>
<% end -%>
